{% if sticky == 1 %}
<header id="header-3" class="header fixed-top header_type {{active_3}}">
{% else %}
<header id="header-3" class="header fixed-top header_type navSticky {{active_3}}">
{% endif %}
	<div class="menubar">
		<div class="container">
			<div class="res-header d-lg-none">
				{{page.logo}}
				<div class="others-in-header">
					<ul class="search-btn">
						<li class="">
							<a class="search-icon" data-bs-toggle="modal" data-bs-target="#searchModal">
							<svg xmlns="http://www.w3.org/2000/svg" width="43.191" height="43.192" viewBox="0 0 43.191 43.192">
								<path  data-name="Icon ionic-ios-search" d="M47.185,44.559,35.173,32.437a17.12,17.12,0,1,0-2.6,2.631L44.508,47.111a1.849,1.849,0,0,0,2.609.067A1.86,1.86,0,0,0,47.185,44.559ZM21.72,35.214a13.516,13.516,0,1,1,9.561-3.958A13.436,13.436,0,0,1,21.72,35.214Z" transform="translate(-4.5 -4.493)"/>
							</svg>
							</a>
						</li>
					</ul>
					{{page.language_block}}
					<button class="open navbar-toggler d-lg-none" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent_3" aria-controls="navbarSupportedContent_3" aria-expanded="false" aria-label="Toggle navigation">
						<svg xmlns="http://www.w3.org/2000/svg" width="240" height="150" viewBox="0 0 240 150">
							<path  data-name="Union 79" d="M19161.313,8514v-21.469H19314V8514Zm-43.656-64.293v-21.422H19314v21.422ZM19074,8385.418V8364h240v21.414Z" transform="translate(-19074 -8364.004)" fill="#181c31"/>
						</svg>
					</button>
					<button class="close navbar-toggler d-none d-lg-none" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent_3" aria-controls="navbarSupportedContent_3" aria-expanded="false" aria-label="Toggle navigation">
						<svg xmlns="http://www.w3.org/2000/svg" width="150" height="150" viewBox="0 0 150 150">
							<path  data-name="Union 80" d="M21.967,128.033A75,75,0,1,1,75,150,75,75,0,0,1,21.967,128.033ZM31.61,31.612A61.363,61.363,0,0,0,75,136.365l0,0A61.364,61.364,0,1,0,31.61,31.612ZM75,84.641,55.718,103.923l-9.641-9.64L65.36,75,46.078,55.718l9.641-9.641L75,65.36,94.283,46.078l9.64,9.641L84.641,75l19.282,19.282-9.64,9.64Z" fill="#181c31"/>
						</svg>
					</button>
				</div>
			</div>	
			<nav class="navbar navbar-expand-lg">
				<div class="header-3-logo d-none d-lg-block">
					{{page.logo}}
				</div>
				<div class="collapse navbar-collapse" id="navbarSupportedContent_3">
					{{page.primary_menu}}
				</div>
				<ul class="search-btn d-none d-lg-block">
					<li class="">
						<a class="search-icon" data-bs-toggle="modal" data-bs-target="#searchModal">
						<svg xmlns="http://www.w3.org/2000/svg" width="43.191" height="43.192" viewBox="0 0 43.191 43.192">
							<path  data-name="Icon ionic-ios-search" d="M47.185,44.559,35.173,32.437a17.12,17.12,0,1,0-2.6,2.631L44.508,47.111a1.849,1.849,0,0,0,2.609.067A1.86,1.86,0,0,0,47.185,44.559ZM21.72,35.214a13.516,13.516,0,1,1,9.561-3.958A13.436,13.436,0,0,1,21.72,35.214Z" transform="translate(-4.5 -4.493)"/>
						</svg>
						</a>
					</li>
				</ul>
				<div class="d-none d-lg-block">
					{{page.language_block}}
				</div>
				<div class="header-contact-number d-none d-lg-block">
					<a href="tel:{{contact_number}}" title="{{contact_number}}">{{contact_number}}</a>
				</div>
				<div class="get-quote-btn d-none d-lg-block">
					<a class="btn btn-primary btn-style-1 btn-sm" href="{{url('<front>')}}{{button_link}}" title="{{button_text}}">{{button_text}}</a>
				</div>
			</nav>
		</div>
	</div>
</header>

