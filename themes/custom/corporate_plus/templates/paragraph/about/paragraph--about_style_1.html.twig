                    </div>
                    <div class="contact">
                        <span>{{content.field_label_1.0}}</span>
                        <a href="tel:{{content.field_contact_number.0}}" title="{{content.field_contact_number.0}}">{{content.field_contact_number.0}}</a>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <img src="{{file_url(content.field_image['#items'].entity.uri.value)}}" alt="{{content.field_image['#items'].alt}}">
                <div class="about-card">
                    <span>{{content.field_text_1.0}}</span>
                    <span>{{content.field_text_2.0}}</span>
                </div>
            </div>
        </div>
    </div>
</section>
