{# 

{{content.field_tagline.0}}
{{content.field_title.0}}
{{content.field_description.0}}
 
{{content.field_link.0['#title']}} #}
<section class="error-page dt-section">
    <div class="container">
        <div class="content">
            <div class="title">{{content.field_title.0}}</div>
            <h2>{{content.field_tagline.0}}</h2>
            <p>{{content.field_description.0}}</p>
            <a class="btn btn-outline-secondary btn-style-3" href="{{content.field_link.0['#url']}}" title="{{content.field_link.0['#title']}}">{{content.field_link.0['#title']}}</a>
            <div class="overlay">
                <img src="{{file_url(content.field_image['#items'].entity.uri.value)}}" alt="{{content.field_image['#items'].alt}}">
            </div>
        </div>
    </div>
</section>