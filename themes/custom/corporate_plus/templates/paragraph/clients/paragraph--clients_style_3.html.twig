<section class="clients-home-style-3">
    <div class="slider-style-5 one">
        <div class="container">
            <div class="clients-3-slider owl-carousel owl-theme">
            {% for key, value in content.field_images['#items'] %}
                <div class="item">
                    <img src="{{file_url(content.field_images['#items'][key].entity.uri.value)}}" alt="{{content.field_images['#items'][key].alt}}">
                </div>
            {% endfor %}
            </div>
        </div>
    </div>
</section>