<section class="get-quote-form dt-section">
	<form{{ attributes.addClass('form-style-1') }}>
		<div class="container">
			<div class="row">  
				<div class="col-lg-6">
				{{ element.elements.name }}
				</div>
				<div class="col-lg-6">
					{{ element.elements.email }}
				</div>
				<div class="col-lg-6">
				{{ element.elements.phone_number }}
				</div>
				<div class="col-lg-6">
				{{ element.elements.country }}
				</div>
				<div class="col-lg-6">
				{{ element.elements.project_type }}
				</div>
				<div class="col-lg-6">
				{{ element.elements.estimate_budget }}
				</div>
				<div class="col-lg-12">
				{{ element.elements.company_name }}
				</div>
				{{ element.elements.required_skills }}
				{{ element.elements.message }}
				{{ element.elements.actions }}
			</div>
			{{ element.form_build_id }}
			{{ element.form_token }}
			{{ element.form_id }}
		</div>
	</form>
</section>